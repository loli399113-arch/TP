    antiActive = not antiActive
    if antiActive then
        AntiScriptButton.Text = "ANTI-SCRIPT: ON"
        AntiScriptButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
    else
        AntiScriptButton.Text = "ANTI-SCRIPT: OFF"
        AntiScriptButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    end
end)

-- Protection en boucle (Nettoyage des objets de lag)
game:GetService("RunService").Heartbeat:Connect(function()
    if antiActive then
        for _, obj in pairs(game.Workspace:GetChildren()) do
            -- Supprime les explosions massives ou sons trop forts créés par des scripts de lag
            if obj:IsA("Explosion") or (obj:IsA("Sound") and obj.Volume > 5) then
                obj:Destroy()
            end
        end
    end
end)
