-- LOGIQUE DE TÉLÉPORTATION
-----------------------------------------------------------
local player = game.Players.LocalPlayer

TPButton.MouseButton1Click:Connect(function()
    local character = player.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        -- Recherche automatique de la base (Tycoon)
        local baseFound = nil
        for _, v in pairs(game.Workspace:GetDescendants()) do
            -- Cherche une base dont le propriétaire est le joueur
            if (v.Name == "Owner" or v.Name == "OwnerName") and v.Value == player.Name then
                baseFound = v.Parent
                break
            end
        end

        if baseFound then
            character.HumanoidRootPart.CFrame = baseFound:GetModelCFrame() + Vector3.new(0, 5, 0)
        else
            -- Si pas trouvé, tente de chercher un objet nommé comme le joueur
            local altBase = game.Workspace:FindFirstChild(player.Name .. "'s Tycoon") or game.Workspace:FindFirstChild("Base")
            if altBase then
                character.HumanoidRootPart.CFrame = altBase.CFrame + Vector3.new(0, 5, 0)
            else
                print("Base non trouvée. Rapproche-toi de ta base une fois pour aider le script.")
            end
        end
    end
end)

